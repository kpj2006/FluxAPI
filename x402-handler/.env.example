# MongoDB Configuration
MONGODB_API_KEY=your_mongodb_api_key
MONGODB_ENDPOINT=https://data.mongodb-api.com/app/your-app-id/endpoint/data/v1
MONGODB_DATABASE_NAME=aipi
MONGODB_DATA_SOURCE=Cluster0

# Solana Configuration
SOLANA_RPC_URL=https://api.devnet.solana.com
SOLANA_CLUSTER=devnet
# For mainnet: https://api.mainnet-beta.solana.com and cluster=mainnet-beta

# Payment Wallet (receives payments for API calls)
SOLANA_PAYMENT_WALLET=YourPublicKeyHere

# Private Key for claiming earnings (base58 or JSON array format)
# Generate with: solana-keygen new
# Export with: solana-keygen export --outfile key.json
SOLANA_PRIVATE_KEY=YourBase58PrivateKeyHere
# Or in JSON array format: [123,45,67,...]

